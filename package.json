{
  "name": "viscum",
  "version": "0.0.1",
  "description": "Multi-agent AI system with Turborepo",
  "type": "module",
  "private": true,
  "workspaces": [
    "server",
    "web",
    "cli"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "clean": "turbo run clean",
    "server": "bun run --filter @viscum/server dev",
    "web": "bun run --filter @viscum/web dev",
    "cli": "bun run --filter @viscum/cli dev",
    "s": "./start.sh",
    "p": "./scripts/performance.sh"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^20.0.0",
    "oxlint": "^0.15.0",
    "prettier": "^3.2.5",
    "turbo": "^2.3.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "bun@1.1.0",
  "engines": {
    "bun": ">=1.0.0"
  }
}
